{
    "version": 2,
    "builds": [
        {
            "src": "server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS",
                "PING"
            ],
            "headers": {
                "Access-Control-Allow-Credentials": "true"
            }
        }
    ]
}
